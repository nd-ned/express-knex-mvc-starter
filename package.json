    "express": "~4.16.1",
    "express-session": "^1.17.1",
    "knex": "^0.21.6",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
