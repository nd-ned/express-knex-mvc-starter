{
  "name": "express-mvc-starter",
  "version": "0.0.1",
  "private": true,
  "author": {
    "name": "Nedko Dimitrov"
  },
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon --inspect ./bin/www",
    "migrate:run": "./node_modules/knex/bin/cli.js migrate:latest",
